# Device specific
SYSTEM=mmcblk1p21
USERDATA=mmcblk1p25
CACHE=mmcblk1p22
BOOT=mmcblk1p15
SYSTEM_FSTYPE=ext3
USERDATA_FSTYPE=ext3
SS_FSTYPE=ext3
SS_PART=mmcblk1p25
SS_DIR=/ss/safestrap
SS_LOC=etc
HIJACK_BIN=logwrapper
HIJACK_LOC=bin
BOOTMODE_PROP=ro.bootmode
DEVICE_PROP=ro.product.device
CHARGER_MODE_SYSFS=/sys/class/power_supply/ac/online
CHECK_BOOTMODE=bp-tools
CHECK_POWERUP_REASON=/sbin/bbx cat /proc/bootinfo | /sbin/bbx grep POWERUPREASON | /sbin/bbx cut -c 19-
POWERUP_REASON_CHARGER=00000100
BACKLIGHT_BRIGHTNESS_PATH=/sys/class/backlight/lm3532_bl/brightness
BACKLIGHT_BRIGHTNESS_VALUE=200
TASKSET_CPUS=0-1
SS_USE_DATAMEDIA=1
SS_COPY_SYSTEM_DATA=0
DEBUG_MODE=0
