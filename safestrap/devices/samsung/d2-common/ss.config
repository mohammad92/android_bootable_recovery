# Device specific
SYSTEM=mmcblk0p14
USERDATA=mmcblk0p15
CACHE=mmcblk0p17
BOOT=mmcblk0p7
SYSTEM_FSTYPE=ext4
USERDATA_FSTYPE=ext4
SS_FSTYPE=ext4
SS_PART=mmcblk0p15
SS_DIR=/ss/safestrap
SS_LOC=etc
HIJACK_BIN=e2fsck
HIJACK_LOC=bin
BOOTMODE_PROP=ro.bootmode
DEVICE_PROP=ro.product.device
CHARGER_MODE_SYSFS=/sys/class/power_supply/ac/online
CHECK_BOOTMODE=bp-tools
CHECK_POWERUP_REASON=/sbin/bbx cat /proc/bootinfo | /sbin/bbx grep POWERUPREASON | /sbin/bbx cut -c 19-
POWERUP_REASON_CHARGER=00000100
BACKLIGHT_BRIGHTNESS_PATH=/sys/devices/platform/msm_fb.524801/leds/lcd-backlight/brightness
BACKLIGHT_BRIGHTNESS_VALUE=200
TASKSET_CPUS=0-1
SS_USE_DATAMEDIA=1
SS_COPY_SYSTEM_DATA=1
DEBUG_MODE=1
