# Device specific
SYSTEM=mmcblk0p22
USERDATA=mmcblk0p23
CACHE=mmcblk0p21
BOOT=mmcblk0p8
SYSTEM_FSTYPE=ext4
USERDATA_FSTYPE=ext4
SS_FSTYPE=ext4
SS_PART=mmcblk0p23
SS_DIR=/ss/safestrap
SS_LOC=etc
HIJACK_BIN=resize_user_data.sh
HIJACK_LOC=etc
BOOTMODE_PROP=ro.bootmode
DEVICE_PROP=ro.product.device
CHARGER_MODE_SYSFS=/sys/class/power_supply/smb349_ac/online
CHECK_BOOTMODE=bp-tools
CHECK_POWERUP_REASON=/sbin/bbx cat /proc/bootinfo | /sbin/bbx grep POWERUPREASON | /sbin/bbx cut -c 19-
POWERUP_REASON_CHARGER=00000100
BACKLIGHT_BRIGHTNESS_PATH=/sys/class/backlight/lcd-backlight/brightness
BACKLIGHT_BRIGHTNESS_VALUE=200
TASKSET_CPUS=0-3
SS_USE_DATAMEDIA=1
SS_COPY_SYSTEM_DATA=1
DEBUG_MODE=1
